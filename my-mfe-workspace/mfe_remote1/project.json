{
  "name": "mfe_remote1",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "mfe_remote1/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/mfe_remote1",
        "index": "mfe_remote1/src/index.html",
        "baseHref": "/",
        "main": "mfe_remote1/src/main.ts",
        "tsConfig": "mfe_remote1/tsconfig.app.json",
        "assets": ["mfe_remote1/src/favicon.ico", "mfe_remote1/src/assets"],
        "styles": ["mfe_remote1/src/styles.css"],
        "scripts": [],
        "webpackConfig": "mfe_remote1/webpack.config.ts",
        "postcssConfig": "mfe_remote1/postcss.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "mfe_remote1/src/environments/environment.ts",
              "with": "mfe_remote1/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "mfe_remote1/webpack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "mfe_remote1:build",
        "hmr": true,
        "port": 4203
      },
      "configurations": {
        "development": {
          "buildTarget": "mfe_remote1:build:development"
        },
        "production": {
          "buildTarget": "mfe_remote1:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "executor": "@nx/react:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "mfe_remote1:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "mfe_remote1:serve:development"
        },
        "production": {
          "serveTarget": "mfe_remote1:serve:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "mfe_remote1/jest.config.ts"
      }
    }
  }
}
