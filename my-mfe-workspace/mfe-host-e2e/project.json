{
  "name": "mfe-host-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "mfe-host-e2e/src",
  "tags": [],
  "implicitDependencies": ["mfe-host"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "mfe-host-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "mfe-host:serve",
        "baseUrl": "http://localhost:4200"
      },
      "configurations": {
        "production": {
          "devServerTarget": "mfe-host:serve:production"
        },
        "ci": {
          "devServerTarget": "mfe-host:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
